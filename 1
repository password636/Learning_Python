shoplist = ['apple', 'mango', 'carrot', 'banana']
hisname = 'swaroop'

print(shoplist[-1])
print(shoplist[-2])
print(shoplist[1:3])
print(shoplist[2:])
print(shoplist[1:-1])
print(shoplist[:])
print(shoplist[::])

print(hisname[0])
print(hisname[1:3])
print(hisname[2:])
print(hisname[1:-1])
print(hisname[:])
print(hisname[::])
print(hisname[:-1])
